//서울에서 김서방 찾기
//String형 배열 seoul의 element중 "Kim"의 위치 x를 찾아, "김서방은 x에 있다"는 String을 반환

class Solution {
    public String solution(String[] seoul) {
        String answer = "";
        
        for (int i=0; i<seoul.length; i++){
            if(seoul[i].equals("Kim")) 
                answer = "김서방은 "+i+"에 있다";
        }   
        return answer;
    }
}
