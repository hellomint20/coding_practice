//문자열 입력받고 문자 하나씩 개행하여 출

import java.util.Scanner;

public class Solution {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String a = sc.next();
        
        for(int i=0; i<a.length(); i++)
            System.out.println(a.charAt(i));
        
    }
}
