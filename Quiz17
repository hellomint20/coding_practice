//공배수
//정수 number와 n, m이 주어집니다. number가 n의 배수이면서 m의 배수이면 1을 아니라면 0을 return

class Solution {
    public int solution(int number, int n, int m) {
        int answer = 0;
        
        if(number % n ==0){
            if(number % m ==0)
                answer = 1;
        }
        else answer =0;
        
        return answer;
    }
}
